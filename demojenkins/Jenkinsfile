node {
    try {
        docker.image('java-build').inside("-i -d -v /webroot:/root -p 9999:80 --name test1"){
            stage ("Check version") {
                sh "java -version"
                sh "mvn -version"
            }
        }
    } catch (e) {
        echo "FAIL"
    } finally {
        echo "SUCCESS"
    }
}




